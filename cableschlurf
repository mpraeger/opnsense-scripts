#!/usr/bin/env bash

export PATH="/sbin:/bin:/usr/sbin:/usr/bin:/usr/local/sbin:/usr/local/bin:/root/bin"

INT='igb2'
V6_PREFIX='2a02'
V6_IP="$(ifconfig igb2 | grep ${V6_PREFIX} | cut -d' ' -f2)"

AFTR='aftr.endpoint.isp.tld'
DNS="$(grep 2a01 /etc/resolv.conf | cut -d' ' -f2 | sort -R| head -n1)"
